    printf("Masukkan waktu keluar (HHMM): ");
    scanf("%d", &waktuKeluar);

    int jenis = kodeParkir / 100000000;
    int waktuMasuk = (kodeParkir / 10000) % 10000;
    int lokasi = kodeParkir % 10000;

